{
  "name": "s-term",
  "version": "0.1.1",
  "description": "Modern SSH Terminal with SFTP support",
  "author": {
    "name": "vdhoangson",
    "email": "vdhoangson@gmail.com",
    "url": "https://github.com/vdhoangson"
  },
  "homepage": "https://github.com/vdhoangson/s-term",
  "license": "MIT",
  "main": "dist-electron/electron/main/index.js",
  "type": "commonjs",
  "scripts": {
    "dev:renderer": "vite",
    "dev:electron": "wait-on tcp:5173 && cross-env ELECTRON_START_URL=http://localhost:5173 electron . --trace-warnings",
    "dev": "concurrently \"npm run dev:renderer\" \"npm run dev:electron\"",
    "build:electron": "tsc -p tsconfig.electron.json",
    "build:renderer": "vite build",
    "start": "electron . --trace-warnings",
    "build": "npm run build:renderer && npm run build:electron && electron-builder",
    "pack": "npm run build:renderer && npm run build:electron && electron-builder --dir",
    "build:win": "electron-builder --win --publish=never",
    "build:mac": "electron-builder --mac --publish=never",
    "build:linux": "electron-builder --linux --publish=never",
    "build:all": "electron-builder -mwl --publish=never",
    "format": "prettier --write \"src/**/*.{js,ts,vue,json,css,scss,md}\" \"electron/**/*.{js,ts}\"",
    "format:check": "prettier --check \"src/**/*.{js,ts,vue,json,css,scss,md}\" \"electron/**/*.{js,ts}\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@xterm/addon-clipboard": "^0.2.0-beta.119",
    "@xterm/addon-fit": "^0.11.0-beta.136",
    "@xterm/addon-image": "^0.9.0-beta.136",
    "@xterm/addon-ligatures": "^0.10.0-beta.136",
    "@xterm/addon-progress": "^0.2.0-beta.42",
    "@xterm/addon-search": "^0.16.0-beta.136",
    "@xterm/addon-serialize": "^0.14.0-beta.136",
    "@xterm/addon-unicode11": "^0.9.0-beta.136",
    "@xterm/addon-webgl": "^0.19.0-beta.136",
    "@xterm/headless": "^5.6.0-beta.136",
    "@xterm/xterm": "^5.6.0-beta.136",
    "electron-log": "^5.4.3",
    "electron-store": "^11.0.2",
    "electron-updater": "^6.6.2",
    "node-gyp": "^12.1.0",
    "node-pty": "1.1.0-beta39",
    "pinia": "^3.0.4",
    "ssh2": "^1.17.0",
    "vue": "^3.5.25",
    "vue-i18n": "^9.14.5",
    "vuedraggable": "^4.1.0",
    "vuetify": "^3.11.1"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/ssh2": "^1.15.5",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.2.3",
    "electron-builder": "^26.0.12",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "^10.6.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-vuetify": "^2.1.2",
    "vue-eslint-parser": "^10.2.0",
    "wait-on": "^7.0.0"
  }
}
